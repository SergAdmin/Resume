<div>

  <main>
    {{> title big=true data=name}}
    {{> line }}

    <section>

      <div class="card">
        <div class="card-body">
          {{#with page_header}}
          <div class="text-center">

            <h1>{{title}}</h1>

            <p class="text-uppercase">Description: {{description}}</p>
          </div>
          {{/with}}

          <div>
            {{#with section}}
            <div>
              {{#with about_us}}
              <div>
                {{#with title_list}}

                <div class="text-end">
                  <p class="display-6">
                    {{main}}
                  </p>
                  <p>
                  <h2>{{sub}}</h2>
                  </p>
                </div>
                {{/with}}
              </div>
              <div>
                <button type="button" class="btn btn-outline-info">{{button.text}}</button>
              </div>


              {{/with}}
            </div>
            {{!-- {{/with}} --}}
          </div>

        </div>
      </div>

      <br>
      <br>

      <div class="card">
        <div class="card-body">
          {{#with advantage}}
          <div>
            <p class="display-5 text-secondary">{{title}}</p>

            <div class="card">
              <div class="card-body">
                <div>
                  {{#with efficiency}}
                  <p class="display-4 text-secondary text-lowercase">{{title}}</p>
                  <div>
                    <h6>{{sub_title}}</h6>
                    <p>{{description}}</p>
                    <button type="button" class="btn btn-outline-warning">{{button}}</button>
                  </div>
                  {{/with}}
                </div>
              </div>
            </div>
          </div>

          <br>
          <br>

          <div class="card">
            <div class="card-body">
              {{#with innovation}}
              <div class="text-center">
                <p class="display-4 text-secondary text-lowercase">{{title}}</p>
                <p>
                <h6>{{sub_title}}</h6>
                </p>
                <p>{{description}}</p>
              </div>

              <div> <button type="button" class="btn btn-outline-warning">{{button}}</button>

              </div>
              {{/with}}
            </div>
          </div>

          <br>
          <br>

          <div class="card">
            <div class="card-body">
              {{#with collaboration}}

              <div class="text-end">
                <p class="display-4 text-secondary text-lowercase">{{title}}</p>

                <h6>{{sub_title}}</h6>
                </p>
                <p>{{description}}</p>
              </div>

              <div>
                <button type="button" class="btn btn-outline-warning">{{button}}</button>
              </div>
              {{/with}}
            </div>
          </div>

        </div>
      </div>




      {{/with}}
</div>
</div>

<br>
<br>

<div class="card">
  <div class="card-body">
    <div>
      <p class="display-5">agreement</p>
      {{#with agreement}}
      <div class="card">
        <div class="card-body">

          <div>
            <h5>{{title}}</h5>
            {{#with info}}
            <div class="card">
              <div class="card-body">

                <div>
                  {{sub_title}}
                  {{#with detail}}
                  <div>
                    <p>{{text}}</p>
                    <button type="button" class="btn btn-success">{{button}}</button>
                  </div>
                  {{/with}}
                </div>
              </div>
            </div>
            {{/with}}
          </div>

        </div>
      </div>
      {{/with}}
    </div>
  </div>
</div>

{{/with}}
</div>

</section>
</main>
</div>