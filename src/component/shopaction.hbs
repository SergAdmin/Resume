<div class="card text-bg-success text-center">
    {{#with service}}
    <h2>{{title}}</h2>
    <div class="card-body">
        <p>{{description}}</p>

        {{#with buttons}}
        {{#each this}}

        <button type="button" href={{link}} class="btn btn-primary">{{text}}</button>
        {{/each}}
        {{/with}}
    </div>
    {{/with}}
</div>